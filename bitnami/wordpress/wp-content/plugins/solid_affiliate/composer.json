{
  "name": "mikeholubowski/affiliate-mvc",
  "config": {
    "platform": {
      "php": "7.3.8"
    }
  },
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "mholubowski",
      "email": "mike.holubowski@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org",
      "only": [
        "wpackagist-plugin/*"
      ]
    }
  ],
  "require": {
    "php": ">=7.3.8",
    "10quality/wp-query-builder": "1.0.12",
    "woocommerce/action-scheduler": "3.4.2",
    "paypal/paypal-payouts-sdk": "1.0.1",
    "seld/jsonlint": "^1.10"
  },
  "require-dev": {
    "lucatume/wp-browser": "3.1.6",
    "codeception/stub": "3.7.0",
    "codeception/module-asserts": "1.3.1",
    "codeception/module-phpbrowser": "1.0.2",
    "codeception/module-webdriver": "1.4.0",
    "codeception/module-db": "1.1.0",
    "codeception/module-filesystem": "1.0.3",
    "codeception/module-cli": "1.1.1",
    "codeception/util-universalframework": "1.0.0",
    "hoa/console": "^3.17",
    "vimeo/psalm": "4.30.0",
    "codeception/module-rest": "1.3.1",
    "lucatume/codeception-snapshot-assertions": "0.2.4",
    "php-stubs/wordpress-stubs": "*",
    "php-stubs/woocommerce-stubs": "*",
    "php-stubs/wp-cli-stubs": "*"
  },
  "autoload": {
    "psr-4": {
      "SolidAffiliate\\Views\\": "assets/Views",
      "SolidAffiliate\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SolidAffiliate\\Tests\\": "tests/_support"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "installer-paths": {
      "libraries/action-scheduler": [
        "woocommerce/action-scheduler"
      ]
    }
  },
  "prefer-stable": true
}
